{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "start": "run-p eleventy:dev sass:dev js:dev js:dev:legacy",
    "build": "run-s eleventy:build sass:build js:build js:build:legacy",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist",
    "js:dev": "esbuild src/js/*.js  src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --watch --sourcemap=inline",
    "js:build": "esbuild src/js/*.js  src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --minify=true",
    "js:dev:legacy": "NODE_ENV=development webpack --progress -w",
    "js:build:legacy": "NODE_ENV=production webpack --progress",
    "clean": "rimraf dist",
    "lint": "run-s lint:html lint:css lint:js",
    "prelint:html": "npm run build",
    "lint:html": "pa11y-ci -c .pa11yci.json './dist/**/*.html'",
    "lint:css": "stylelint 'src/scss/**/*.scss' --color --formatter verbose",
    "lint:js": "eslint . --ext .js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": [
    ">0%",
    "not dead",
    "not supports es6-module"
  ]
}
