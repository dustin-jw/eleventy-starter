{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prestart": "check-node-version --package && npm run clean",
    "prebuild": "check-node-version --package && npm run clean",
    "start": "run-p eleventy:dev sass:dev js:dev js:dev:legacy",
    "build": "run-s eleventy:build sass:build js:build js:build:legacy",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist",
    "js:dev": "esbuild src/js/*.js  src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --watch --sourcemap=inline",
    "js:build": "esbuild src/js/*.js  src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --minify=true",
    "js:dev:legacy": "NODE_ENV=development webpack --progress -w",
    "js:build:legacy": "NODE_ENV=production webpack --progress",
    "clean": "rm -rf dist",
    "lint": "run-s lint:css lint:js",
    "prelint:html": "npm run build",
    "lint:css": "stylelint 'src/scss/**/*.scss' --color --formatter verbose",
    "lint:js": "eslint . --ext .js,.ts",
    "test": "vitest --run",
    "update-deps": "ncu -u",
    "postupdate-deps": "npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.30.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-loader": "^9.1.2",
    "check-node-version": "^4.2.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "npm-check-updates": "^16.6.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": [
    ">0%",
    "not dead",
    "not supports es6-module"
  ]
}
